const formatCount = (count) => {
  if (typeof count === 'string') {
    count = parseInt(count)
  }
  if (count > 100000000) {
    return Math.round(count / 10000000) / 10 + '亿'
  }
  if (count > 10000) {
    return Math.round(count / 1000) / 10 + '万'
  }
  return count + ''
}
